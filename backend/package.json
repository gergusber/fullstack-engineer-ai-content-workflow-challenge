{
  "name": "backend",
  "version": "1.0.0",
  "description": "AI Content Workflow Backend",
  "main": "dist/main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- -d src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- -d src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- -d src/database/data-source.ts migration:revert",
    "schema:sync": "npm run typeorm -- -d src/database/data-source.ts schema:sync",
    "schema:drop": "npm run typeorm -- -d src/database/data-source.ts schema:drop",
    "seed": "ts-node src/database/seeds/seed.ts"
  },
  "keywords": [
    "nestjs",
    "ai",
    "content",
    "workflow"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.63.0",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "openai": "^5.20.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.1",
    "@types/pg": "^8.15.5",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
